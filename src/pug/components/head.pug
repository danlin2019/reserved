- var title = '五十甲建築行銷團隊'
- var description = '.'
- var d = Date.now()

title #{title}
meta(charset='UTF-8')
meta(lang='tw')
meta(http-equiv='X-UA-Compatible', content='IE=edge,chrome=1')
// viewport
meta#viewport(name='viewport', content='width=device-width, initial-scale=1, maximum-scale=1')
// SEO
meta(name="title" content=title)
meta(name='description', content=description)
meta(name='keywords', content='JWT')
// facebook
meta(property='og:title', content=title)
meta(property='og:description', content=description)
meta(property='og:image', content='')
meta(property='og:url', content='')
meta(property='og:type', content='website')
meta(property='og:site_name', content='JWT')
// favicon
link(rel='shortcut icon', type='image/x-icon', href='files/img/icon-tag.ico')
link(rel='icon', type='image/x-icon', href='files/img/icon-tag.png')
//- link(rel='bookmark', type='image/x-icon', href='')

link(rel='stylesheet', href='files/css/main.css?v=' + d)

